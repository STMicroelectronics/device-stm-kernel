From a642aad928deb337eb801ea747fb2acfa9fe90fa Mon Sep 17 00:00:00 2001
From: Yves COPPEAUX <yves.coppeaux@st.com>
Date: Wed, 26 Jun 2024 09:55:50 +0200
Subject: [PATCH 13/33] dts: android: set usb role to peripheral as default

---
 arch/arm64/boot/dts/st/stm32mp257f-ev1.dts | 1 +
 1 file changed, 1 insertion(+)

diff --git a/arch/arm64/boot/dts/st/stm32mp257f-ev1.dts b/arch/arm64/boot/dts/st/stm32mp257f-ev1.dts
index 76581fa7fc043..e1de8e97b097f 100644
--- a/arch/arm64/boot/dts/st/stm32mp257f-ev1.dts
+++ b/arch/arm64/boot/dts/st/stm32mp257f-ev1.dts
@@ -829,6 +829,7 @@ &usb3dr {
 	dwc3: usb@48300000 {
 		maximum-speed = "high-speed";
 		usb-role-switch;
+		role-switch-default-mode = "peripheral";
 		port {
 			dwc3_ep: endpoint {
 				remote-endpoint = <&typec_ep>;
